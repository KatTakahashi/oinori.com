<div class="bg-img-form">
  <section class="login-admin wrapper">
    <h2 class="section-title white">管理者ログイン</h2>

    <%= form_with model: @session, local: true do |f| %>
      <%= flash[:danger] %>
      <table id="form-admin">
        <tr>
          <th class="white"><%= f.label :namae, '名前' %></th>
          <td><%= f.text_field :name %></td>
        </tr>
        <tr>
          <th class="white"><%= f.label :password, 'パスワード' %></th>
          <td><%= f.password_field :password %></td>
        </tr>
        <tr>
          <td colspan="2"><%= f.submit 'ログイン' %></td>
        </tr>
      </table>
    <% end %>
  </section>
</div>